{
  "name": "t3v/t3v_datamapper",
  "type": "typo3-cms-extension",
  "description": "The Data Mapper extension of TYPO3voilà.",
  "authors": [
    {
      "name": "Maik Kempe",
      "email": "mkempe@bitaculous.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "CMS",
    "Data",
    "Database",
    "Extension",
    "Laravel",
    "Mapper",
    "ORM",
    "SQL",
    "T3v",
    "TYPO3",
    "TYPO3voilà",
    "UH LÁLÁ",
    "Validation"
  ],
  "homepage": "https://t3v.github.io/t3v_datamapper/",
  "support": {
    "issues": "https://github.com/t3v/t3v_datamapper/issues"
  },
  "license": "MIT",
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "bin-dir": ".build/bin",
    "vendor-dir": ".build/vendor"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "replace": {
    "typo3-ter/t3v_datamapper": "self.version"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }
  ],
  "require": {
    "php": ">= 7.2",
    "typo3/cms-core": "^9.5.31 || ^10.4.31 || ^11.5.13",
    "phplucidframe/console-table": "^1.2",
    "illuminate/database": "^6.20 || ^8.83 || ^9.20",
    "illuminate/validation": "^6.20 || ^8.83 || ^9.20",
    "t3v/t3v_core": "dev-develop"
  },
  "require-dev": {
    "ext-dom": "*",
    "t3v/t3v_testing": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "T3v\\T3vDataMapper\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "T3v\\T3vTesting\\Tests\\": ".build/web/typo3conf/ext/t3v_testing/Tests/",
      "T3v\\T3vDataMapper\\Tests\\": "Tests"
    }
  },
  "extra": {
    "typo3/cms": {
      "app-dir": ".build",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "extension-key": "t3v_datamapper",
      "web-dir": ".build/web"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .build/web/typo3conf/ext/",
      "[ -L .build/web/typo3conf/ext/t3v_datamapper ] || ln -snvf ../../../../. .build/web/typo3conf/ext/t3v_datamapper"
    ]
  }
}
